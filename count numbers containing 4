class Solution {
    public static int countNumberswith4(int n) {
        // code here
        int res=0;
        for(int i=4;i<=n;i++){
            int j=i;
            while(j>0){
                if(j%10==4){
                    res++;
                    break;
                }
                j=j/10;
            }
        }
        return res;
    }
}
